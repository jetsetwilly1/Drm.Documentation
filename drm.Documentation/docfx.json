{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "./"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "sitemap": {
      "baseUrl": "https://docs.drmtemplates.io",
      "priority": 0.1,
      "changefreq": "monthly",
      "fileOptions": {
        "tutorials/**.md": {
          "priority": 0.5,
          "changefreq": "daily"
        },
        "documentation/**.md": {
          "priority": 0.3,
          "changefreq": "monthly"
        },
        "drmtemplates/**.md": {
          "priority": 0.5,
          "changefreq": "daily"
        },
        "articles/**.md": {
          "priority": 0.8,
          "changefreq": "daily"
        }
      }
    },
    "content": [
      {
        "files": [
          "articles/**.md",
          "tutorials/**.md",
          "tutorials/**/toc.yml",
          "reference/**.md",
          "reference/**/toc.yml",
          "drmtemplates/**.md",
          "drmtemplates/**/toc.yml",
          "documentation/**.md",
          "documentation/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "logo.svg",
          "favicon.ico",
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Automate Dynamics 365 Deployments",
      "_enableSearch": true,
      "_baseUrl": "https://docs.drmtemplates.io"
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default", "_exported_templates/modern" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
